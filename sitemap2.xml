---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% assign filtered_repos = site.github.public_repositories | where:'archived', false | where:'fork', false | sort: 'pushed_at' | reverse %}

{% for repository in filtered_repos | limit: site.projects.limit %}
    {% unless site.projects.exclude.projects contains repository.name %}
      {% if repository.homepage != "" && if repository.homepage != nil %}
      <url>
      <loc>{{ repository.homepage }}</loc>
      <lastmod>{{ repository.pushed_at | date_to_xmlschema }}</lastmod>
      <changefreq>daily</changefreq>
      <priority>1</priority>
      </url>
      {% endif %}
    {% endunless %}
  {% endfor %}
</urlset>